pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        script {
          echo 'Building...'
        }
      }
    }
    
    stage('Test') {
      steps {
        script {
          echo 'Testing...'
        }
      }
    }
    
    stage('Deploy') {
      steps {
        script {
          echo 'Deploying...'
        }
      }
    }
    
    stage('Cleanup') {
      steps {
        script {
          echo 'Performing cleanup...'
        }
      }
    }
  }
}
